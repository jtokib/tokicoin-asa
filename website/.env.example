# TokiCoin Website Environment Variables Template
# Copy to .env.local and customize for your setup

# Asset Configuration
VITE_ASSET_ID=your_asset_id_here
VITE_NETWORK=mainnet
VITE_ASSET_NAME=TokiCoin
VITE_ASSET_SYMBOL=TOKI

# Website Configuration
VITE_WEBSITE_URL=https://your-tokicoin-site.netlify.app
VITE_GITHUB_URL=https://github.com/your-username/tokicoin-asa
VITE_EXPLORER_BASE_URL=https://algoexplorer.io

# Feature Flags
VITE_ENABLE_COPY_CLIPBOARD=true
VITE_ENABLE_WALLET_INTEGRATION=true
VITE_ENABLE_REAL_TIME_DATA=false

# Development Settings
VITE_DEBUG_MODE=false